{
  "name": "bsh-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract:i18n": "formatjs extract '{pages,components,containers}/**/*.{js,ts,tsx}' --ignore='**/*.d.ts' --out-file lang/extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format formatter.js",
    "compile:i18n": "formatjs compile-folder --ast lang/extracted lang/compiled --format formatter.js",
    "i18n": "npm run extract:i18n && npm run compile:i18n",
    "format": "prettier --write"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "1.0.6",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "classnames": "^2.3.2",
    "eslint": "^8.27.0",
    "eslint-config-next": "13.0.3",
    "next": "13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.2.1",
    "react-select": "^5.6.1",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.3",
    "@next/eslint-plugin-next": "^13.0.3",
    "@svgr/webpack": "^6.5.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "autoprefixer": "^10.4.13",
    "babel-plugin-formatjs": "^10.3.31",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "husky": "4",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.3"
  },
  "resolutions": {
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "next": "13.0.3",
    "@heroicons/react": "1.0.6",
    "husky": "4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
